generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.sqlite"
}

model token {
  uid        Int          @id @unique
  token      String?
  paste      String
  lastUsed   DateTime     @default(now())
  message    String?
  paintEvent paintEvent[]
}

model paintEvent {
  id     Int    @id @default(autoincrement())
  pos    Int
  rgb    Int
  uid    Int?
  token  token? @relation(fields: [uid], references: [uid])
  status Int
  result Int?
  random Float
}
